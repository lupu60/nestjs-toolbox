{
  "name": "@nest-toolbox/typeorm-audit-log",
  "description": "Automatic audit logging for TypeORM entities with user attribution and diff tracking",
  "version": "1.8.1",
  "author": "bogdanlupu <lupu60@gmail.com>",
  "homepage": "https://github.com/lupu60/nestjs-toolbox/tree/master/packages/typeorm-audit-log#readme",
  "license": "MIT",
  "keywords": [
    "typeorm",
    "audit-log",
    "audit-trail",
    "nestjs",
    "orm",
    "compliance",
    "toolbox"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lupu60/nestjs-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/lupu60/nestjs-toolbox/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=19 <=22",
    "npm": "<=11"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "biome check src",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "deep-diff": "^1.0.2",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.26"
  },
  "devDependencies": {
    "@types/deep-diff": "^1.0.5",
    "@types/node": "24.0.3",
    "rimraf": "6.0.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "typeorm": "^0.3.0"
  }
}
